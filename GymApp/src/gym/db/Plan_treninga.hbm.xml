<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>

	<class name="gym.db.Plan_treninga" table="plan_treninga">		
		<id name="id" column="id">
        	<generator class="native"/>
        </id>				
		<property name="broj_ponavljanja"/>
		<many-to-one name="vjezba" column="id_vjezbe" lazy="false" />
		<many-to-one name="trenig" column="id_treninga" lazy="false" />	
		<property name="tezina"/>		
		<property name="dan"/>			
	</class>

</hibernate-mapping>